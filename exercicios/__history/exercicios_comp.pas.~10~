   if (anonasc<1820) then
   begin
     MessageDlg('Ops, o número que voçê digitou não corresponde a um ano do tempo atual!', mtWarning,[mbYes],0);
     exit;
   end;
   if (anonasc>2022) then
   begin
     MessageDlg('Ops, o número que voçê digitou não corresponde a um ano ja corrido!', mtWarning,[mbYes],0);
     exit;
   end;
idade:= 2021 - anonasc;
ShowMessage('A sua idade é: '+inttostr(idade)+' anos');
end;

