// exercicio 08
procedure TForm1.Button8Click(Sender: TObject);
var
  verificar01, total, total01: single;
begin
  verificar01 := StrToFloat(Editexer_08.Text);
  total := sqrt(verificar01);
  if total > 0 then
  begin
    ShowMessage('A raiz quadrado do número ' + FloatToStr(verificar01) +
      ' é positiva ' + FloatToStr(total));
  end
  else if (total < 0) then
  begin
    total01 := total * total;
    ShowMessage('A raiz quadrado do número ' + FloatToStr(verificar01) +
      ' é negativa, elevado ao qudrado é ' + CurrToStr(total01));
  end;

end;

