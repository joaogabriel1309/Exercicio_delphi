    ShowMessage('O número ' + inttostr(verifica) + ' é iqual a 20');
  end;
end;

procedure TForm1.CalcularClick(Sender: TObject);
var
salario,salariocal,total: Currency;
begin
salario:= StrTocurr(Editexer31.Text);

  if (salario<= 600.00) then
  begin
    ShowMessage('Isento do INSS');
    exit;
  end;
  if (salario>600.00) and (salario<=1200.00) then
  begin
    total:= salario * 0.2;
    salariocal:= salario-total;
    ShowMessage('Desconto de - 20% total: R$'+CurrToStr(salariocal));
    exit;
  end;
  if (salario>1200.00) and (salario<=2000.00) then
  begin
    total:= salario*0.25;
    salariocal:= salario-total;
    ShowMessage('Desconto de - 25% total: R$'+CurrToStr(salariocal));
  end
  else
  begin
    total:= salario*0.30;
    salariocal:= salario-total;
    ShowMessage('Desconto de - 30% total: R$'+CurrToStr(salariocal));
  end;

end;



function TForm1.dividir(cat_op, cat_ad, hip: Currency): Currency;
var seno,coseno: Currency;
begin
seno := (cat_op / hip);
coseno := (cat_ad / hip);

end;

procedure TForm1.OrdenarClick(Sender: TObject);
var
  v1, v2, v3, v4, v5: integer;
begin
  v1 := strtoint(Editexer26_v1.Text);
  v2 := strtoint(Editexer26_v2.Text);
  v3 := strtoint(Editexer26_v3.Text);
  v4 := strtoint(Editexer26_v4.Text);
  v5 := strtoint(Editexer26_v5.Text);
  Memo26.Lines.Clear;

  if (v1 = v2) or (v1 = v3) or (v1 = v4) or (v1 = v5) or (v2 = v1) or (v2 = v3)
    or (v2 = v4) or (v2 = v5) or (v3 = v2) or (v3 = v1) or (v3 = v4) or
    (v3 = v5) or (v4 = v2) or (v4 = v3) or (v4 = v1) or (v4 = v5) or (v5 = v2)
    or (v5 = v3) or (v5 = v4) or (v5 = v1) then
  begin
    Messagedlg('Voçê inserio valores iguais!', mtWarning, [mbOK], 0)
  end;

  if (v1 > v2) and (v1 > v3) and (v1 > v4) and (v1 > v5) then
  begin
    if (v2 > v3) and (v2 > v4) and (v2 > v5) then
    begin
      if (v3 > v4) and (v3 > v5) then
      begin
        if v4 > v5 then
        begin
          Memo26.Lines.Add(inttostr(v1));
          Memo26.Lines.Add(inttostr(v2));
          Memo26.Lines.Add(inttostr(v3));
          Memo26.Lines.Add(inttostr(v4));
          Memo26.Lines.Add(inttostr(v5));
        end;
        if v5 > v4 then
        begin
          Memo26.Lines.Add(inttostr(v1));
          Memo26.Lines.Add(inttostr(v2));
          Memo26.Lines.Add(inttostr(v3));
          Memo26.Lines.Add(inttostr(v5));
          Memo26.Lines.Add(inttostr(v4));
        end;
      end;
      if (v4 > v3) and (v4 > v5) then
      begin
        if v3 > v5 then
        begin
          Memo26.Lines.Add(inttostr(v1));
          Memo26.Lines.Add(inttostr(v2));
          Memo26.Lines.Add(inttostr(v4));
          Memo26.Lines.Add(inttostr(v3));
          Memo26.Lines.Add(inttostr(v5));
        end;
        if v5 > v3 then
        begin
          Memo26.Lines.Add(inttostr(v1));
          Memo26.Lines.Add(inttostr(v2));
          Memo26.Lines.Add(inttostr(v4));
          Memo26.Lines.Add(inttostr(v5));
          Memo26.Lines.Add(inttostr(v3));
        end;
      end;
      if (v5 > v3) and (v5 > v4) then
      begin
        if v3 > v4 then
        begin
          Memo26.Lines.Add(inttostr(v1));
          Memo26.Lines.Add(inttostr(v2));
          Memo26.Lines.Add(inttostr(v5));
          Memo26.Lines.Add(inttostr(v3));
          Memo26.Lines.Add(inttostr(v4));
        end;
