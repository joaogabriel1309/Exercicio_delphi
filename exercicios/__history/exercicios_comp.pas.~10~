// exercicio 04
procedure TForm1.Button40Click(Sender: TObject);
var nome: string; valor, lucro: Currency;
begin
nome:= Editexer44_nome.Text;
valor:= StrToCurr(Editexer44_valor.Text);
  if (valor < 10.00) then
  begin
    lucro:= valor*0.70;
    ShowMessage('O lucro do produto '+nome+' na venda vai ser de: R$'+currtostr(lucro));
  end else if (valor > 10) and (valor < 30) then
  begin
    lucro:= valor*0.50;
    ShowMessage('O lucro do produto '+nome+' na venda vai ser de: R$'+currtostr(lucro));
  end else if (valor > 30) and (valor < 50) then
  begin
    lucro:= valor*0.40;
    ShowMessage('O lucro do produto '+nome+' na venda vai ser de: R$'+currtostr(lucro));
  end
  else
  begin
    lucro:= valor*0.03;
    ShowMessage('O lucro do produto '+nome+' na venda vai ser de: R$'+currtostr(lucro));
  end;
end;

procedure TForm1.Button41Click(Sender: TObject);
var cat_adj,cat_ops,hipot,seno,coseno,seno1,coseno1: Currency;
a,b:integer;
begin
cat_adj:= StrTocurr(Editexer46_angulo1.Text);
cat_ops:= StrTocurr(Editexer46_angulo2.Text);
hipot:= StrTocurr(Editexer46_angulo3.Text);
seno1 := seno mod 2;
coseno1 := coseno mod 2;
  if (seno1 = 0) then
  begin
    Memo46.Lines.Add('O ângolo seno e par');
      if (seno <= 90) then
      begin
        memo46.Lines.Add('O angulo esta no 1° quadrante');
      end;
      if (seno <= 180) then
      begin
        memo46.Lines.Add('O angulo esta no 2° quadrante');
      end;
      if (seno <= 270) then
      begin
        memo46.Lines.Add('O angulo esta no 3° quadrante');
      end;
      if (seno <= 360) then
      begin
        memo46.Lines.Add('O angulo esta no 4° quadrante');
      end;
  end;
  if (coseno1 = 1) then
  begin
    Memo46.Lines.Add('O ângolo conseno e impar e esta no ');
    if (coseno <= 90) then
      begin
        memo46.Lines.Add('O angulo esta no 1° quadrante');
      end;
      if (coseno <= 180) then
      begin
        memo46.Lines.Add('O angulo esta no 2° quadrante');
      end;
      if (coseno <= 270) then
      begin
        memo46.Lines.Add('O angulo esta no 3° quadrante');
      end;
      if (coseno <= 360) then
      begin
        memo46.Lines.Add('O angulo esta no 4° quadrante');
      end;
  end;

end;

