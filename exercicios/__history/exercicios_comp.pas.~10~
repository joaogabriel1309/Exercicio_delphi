function TForm1.verificamenormaior3(valor1, valor2, valor3: integer): integer;
begin
  if (valor1 > valor2) and (valor2 > valor3) then
  begin
    result := valor1 + valor2 + valor3;
  end;
  if (valor2 > valor1) and (valor1 > valor3) then
  begin
    result := valor2 + valor1 + valor3;
  end;
  if (valor1 > valor3) and (valor3 > valor2) then
  begin
    result := valor1 + valor3 + valor2;
  end;
  if (valor3 > valor2) and (valor2 > valor1) then
  begin
    result := valor3 + valor2 + valor1;
  end;
  if (valor2 > valor3) and (valor3 > valor1) then
  begin
    result := valor2 + valor3 + valor1;
  end;
end;

end.
