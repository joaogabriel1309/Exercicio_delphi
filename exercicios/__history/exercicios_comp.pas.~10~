// exercicio10
procedure TForm1.AnalisarClick(Sender: TObject);
var
  a, b, c: integer;
begin
a:= strtoint(Editexer27_v1.Text);
b:= strtoint(Editexer27_v2.Text);
c:= strtoint(Editexer27_v3.Text);

  if (a = b) and (a = c) and (b = c) then
  begin
    ShowMessage('Podemos forma um triangulo "equilatero"');
  end
  else
  if (a=b) or (a=c) or (b=c) then
  begin
    ShowMessage('Podemos forma um triangulo "isosceles"');
  end
  else
  begin
    ShowMessage('Podemos forma um triangulo "escaleno"');
  end;
end;

procedure TForm1.Button10Click(Sender: TObject);
var
  verifica: integer;
begin
  verifica := strtoint(editexer10.Text) mod 2;
  if verifica = 1 then
  begin
    ShowMessage('O valor do digito da dezena e impar');
  end
  else
  begin
    ShowMessage('O valor do digito da dezena e par');
  end;

end;

procedure TForm1.Button11Click(Sender: TObject);
var
  Verificar, total: Currency;
begin
  Verificar := StrToCurr(Editexer11.Text);
  total := sqrt(Verificar);
  if frac(total) = 0.00 then
  begin
    ShowMessage('A raiz do número ' + CurrToStr(Verificar) + ' é ' +
      CurrToStr(total) + ', é inteira');
  end
  else
    ShowMessage('A raiz do número ' + CurrToStr(Verificar) + ' é ' +
      CurrToStr(total) + ', não é inteira');
end;

procedure TForm1.Button12Click(Sender: TObject);
var
  nome: string;
  pr1, pr2, media: Currency;
begin
  nome := Editexer12_nome.Text;
  pr1 := StrToCurr(editexer12_pr1.Text);
  pr2 := StrToCurr(Editexer12_pr2.Text);
  media := (pr1 + pr2) / 2;
  Memo1.Clear;
  if media >= 7 then
  begin
    Memo1.Lines.Add('Nome do aluno: ' + nome);
    Memo1.Lines.Add('O aluno esta APROVADO, a media do aluno foi ' +
      CurrToStr(media));
    exit;
  end
  else if media <= 3 then
  begin
    Memo1.Lines.Add('Nome do aluno: ' + nome);
    Memo1.Lines.Add('O aluno esta REPROVADO, a media do aluno foi ' +
      CurrToStr(media));
    exit;
  end
  else
    Memo1.Lines.Add('Nome do aluno: ' + nome);
  Memo1.Lines.Add('O aluno esta na PROVA FINAL, a media do aluno foi ' +
    CurrToStr(media));
end;

procedure TForm1.Button13Click(Sender: TObject);
var
  Verificar, v01, v02: integer;
begin
  v01 := strtoint(Editexer14_v01.Text);
  v02 := strtoint(Editexer14_v02.Text);
  Verificar := v01 mod v02;
  if Verificar = 0 then
  begin
    ShowMessage('O número ' + inttostr(v01) + ' é divisivel por ' +
      inttostr(v02));
  end
  else
  begin
    ShowMessage('O número ' + inttostr(v01) + ' NÃO é divisivel por ' +
      inttostr(v02));
  end;

end;

procedure TForm1.Button14Click(Sender: TObject);
var
  valo1, valo2, resul: Currency;
begin
  valo1 := StrToCurr(Editexer15_dividendo.Text);
  valo2 := StrToCurr(Editexer15_quociente.Text);
  resul := valo2 / valo1;
  if frac(resul) = 0.00 then
  begin
    ShowMessage('O quonciente e inteiro! valor: ' + CurrToStr(resul));
  end
  else
  begin
    ShowMessage('O quonciente não e inteiro! valor: ' + CurrToStr(resul));
  end;
end;

procedure TForm1.Button15Click(Sender: TObject);
var
  v1, v2: integer;
begin
  v1 := strtoint(Editexer16_v01.Text);
  v2 := strtoint(Editexer16_v02.Text);
  if v1 = v2 then
  begin
    ShowMessage('Valores iquais!');
  end
  else
  begin
    ShowMessage('Valores diferentes!');
  end;

end;

procedure TForm1.Button16Click(Sender: TObject);
var
  v1, v2: integer;
begin
  v1 := strtoint(Editexer17_v1.Text);
  v2 := strtoint(editexer17_v2.Text);
  if v1 > v2 then
  begin
    ShowMessage('O maior número é : ' + inttostr(v1));
  end
  else
  begin
    ShowMessage('O maior número é : ' + inttostr(v2));
  end;

end;

procedure TForm1.Button17Click(Sender: TObject);
var
  v1, v2: integer;
begin
  v1 := strtoint(Editexer18_v1.Text);
  v2 := strtoint(Editexer18_v2.Text);
  if v1 < v2 then
  begin
    ShowMessage('O menor número é : ' + inttostr(v1));
  end
  else
  begin
    ShowMessage('O menor número é : ' + inttostr(v2));
  end;

end;

procedure TForm1.Button18Click(Sender: TObject);
var
  v1, v2, resu: integer;
begin
  v1 := strtoint(Editexer19_v1.Text);
  v2 := strtoint(Editexer19_v2.Text);
  Memoexer19.Clear;

  if v1 > v2 then
  begin
    Memoexer19.Lines.Add(inttostr(v1));
    Memoexer19.Lines.Add(inttostr(v2));
  end
  else
  begin
    Memoexer19.Lines.Add(inttostr(v2));
    Memoexer19.Lines.Add(inttostr(v1));
  end;
end;

procedure TForm1.Button19Click(Sender: TObject);
var
  v1, v2, resu: integer;
begin
  v1 := strtoint(Editexer20_v1.Text);
  v2 := strtoint(Editexer20_v2.Text);
  Memoexer20.Clear;

  if v1 < v2 then
  begin
    Memoexer20.Lines.Add(inttostr(v1));
    Memoexer20.Lines.Add(inttostr(v2));
  end
  else
  begin
    Memoexer20.Lines.Add(inttostr(v2));
    Memoexer20.Lines.Add(inttostr(v1));
  end;
end;

procedure TForm1.VerificarClick(Sender: TObject);
var
  nome: string;
begin
  nome := Editexer13.Text;
  if (nome = 'brasília') OR (nome = 'BRASÍLIA') then
  begin
    ShowMessage('PARABÉNS, você acertou! ');
  end
  else
  begin;
    ShowMessage('ERROU, tente novamente!');
  end;
end;

// exercicio 1
procedure TForm1.Button1Click(Sender: TObject);
var
  receb: integer;
begin
  receb := strtoint(recebenumero.Text);
  if (receb > 20) then
  begin
    ShowMessage(inttostr(receb));
  end
  else
  begin
    ShowMessage('erro numero menor que 20!');
  end;

end;

// exercicio 02
procedure TForm1.Button20Click(Sender: TObject);
var
  v1, v2, v3: integer;
begin
  v1 := strtoint(Editexer21_v1.Text);
  v2 := strtoint(Editexer21_v2.Text);
  v3 := strtoint(Editexer21_v3.Text);

  if v1 >= v2 then
  begin
    if v1 >= v3 then
    begin
      ShowMessage('O maior número digito é: ' + inttostr(v1));
    end;
  end;
  if v2 >= v1 then
  begin
    if v2 >= v3 then
    begin
      ShowMessage('O maior número digito é: ' + inttostr(v2));
    end;
  end;
  if v3 > v1 then
  begin
    if v3 > v2 then
    begin
      ShowMessage('O maior número digito é: ' + inttostr(v3));
    end;
  end;
end;

procedure TForm1.Button21Click(Sender: TObject);
var
  v1, v2, v3: integer;
begin
  v1 := strtoint(Editexer22_v01.Text);
  v2 := strtoint(Editexer22_v02.Text);
  v3 := strtoint(Editexer22_v03.Text);

  if v1 >= v2 then
  begin
    if v1 >= v3 then
    begin
      ShowMessage('O maior número digito é: ' + inttostr(v1));
    end;
  end;
  if v2 >= v1 then
  begin
    if v2 >= v3 then
    begin
      ShowMessage('O maior número digito é: ' + inttostr(v2));
    end;
  end;
  if v3 > v1 then
  begin
    if v3 > v2 then
    begin
      ShowMessage('O maior número digito é: ' + inttostr(v3));
    end;
  end;
end;

procedure TForm1.Button22Click(Sender: TObject);
var
  v1, v2, v3: integer;
begin
  v1 := strtoint(Editexer23_v1.Text);
  v2 := strtoint(Editexer23_v2.Text);
  v3 := strtoint(Editexer23_v3.Text);
  caixadetexto.Clear;

  if (v1 < v2) then
  begin
    if (v1 < v3) then
    begin
      if (v2 < v3) then
      begin
        caixadetexto.Lines.Add(inttostr(v1));
        caixadetexto.Lines.Add(inttostr(v2));
        caixadetexto.Lines.Add(inttostr(v3));
      end;
      if v3 < v2 then
      begin
        caixadetexto.Lines.Add(inttostr(v1));
        caixadetexto.Lines.Add(inttostr(v3));
        caixadetexto.Lines.Add(inttostr(v2));
      end;
    end;
  end;
  if v2 < v1 then
  begin
    if v2 < v3 then
    begin
      if v1 < v3 then
      begin
        caixadetexto.Lines.Add(inttostr(v2));
        caixadetexto.Lines.Add(inttostr(v1));
        caixadetexto.Lines.Add(inttostr(v3));
      end
      else
      begin
        caixadetexto.Lines.Add(inttostr(v2));
        caixadetexto.Lines.Add(inttostr(v3));
        caixadetexto.Lines.Add(inttostr(v1));
      end;
    end;
  end;
  if v3 < v2 then
  begin
    if v3 < v1 then
    begin
      if v1 < v2 then
      begin
        caixadetexto.Lines.Add(inttostr(v3));
        caixadetexto.Lines.Add(inttostr(v1));
        caixadetexto.Lines.Add(inttostr(v2));
      end;
      if v2 < v1 then
      begin
        caixadetexto.Lines.Add(inttostr(v3));
        caixadetexto.Lines.Add(inttostr(v2));
        caixadetexto.Lines.Add(inttostr(v1));
      end;
    end;
  end;
end;

procedure TForm1.Button23Click(Sender: TObject);
var
  v1, v2, v3: integer;
begin
  v1 := strtoint(Editexer24_v1.Text);
  v2 := strtoint(Editexer24_v2.Text);
  v3 := strtoint(Editexer24_v3.Text);
  Memo24.Clear;

  if (v1 > v2) then
  begin
    if (v1 > v3) then
    begin
      if (v2 > v3) then
      begin
        Memo24.Lines.Add(inttostr(v1));
        Memo24.Lines.Add(inttostr(v2));
        Memo24.Lines.Add(inttostr(v3));
      end;
      if v3 > v2 then
      begin
        Memo24.Lines.Add(inttostr(v1));
        Memo24.Lines.Add(inttostr(v3));
        Memo24.Lines.Add(inttostr(v2));
      end;
    end;
  end;
  if v2 > v1 then
  begin
    if v2 > v3 then
    begin
      if v1 > v3 then
      begin
        Memo24.Lines.Add(inttostr(v2));
        Memo24.Lines.Add(inttostr(v1));
        Memo24.Lines.Add(inttostr(v3));
      end
      else
      begin
        Memo24.Lines.Add(inttostr(v2));
        Memo24.Lines.Add(inttostr(v3));
        Memo24.Lines.Add(inttostr(v1));
      end;
    end;
  end;
  if v3 > v2 then
  begin
    if v3 > v1 then
    begin
      if v1 > v2 then
      begin
        Memo24.Lines.Add(inttostr(v3));
        Memo24.Lines.Add(inttostr(v1));
        Memo24.Lines.Add(inttostr(v2));
      end;
      if v2 > v1 then
      begin
        Memo24.Lines.Add(inttostr(v3));
        Memo24.Lines.Add(inttostr(v2));
        Memo24.Lines.Add(inttostr(v1));
      end;
    end;
  end;
end;

procedure TForm1.Button24Click(Sender: TObject);
var
  v1, v2, v3: integer;
begin
  v1 := strtoint(Editexer25_v1.Text);
  v2 := strtoint(Editexer25_v2.Text);
  v3 := strtoint(Editexer25_v3.Text);

  if (v1 > v2) then
  begin
    if (v1 > v3) then
    begin
      if (v2 > v3) then
      begin
        editexer25_rece1.Text := (inttostr(v1));
        editexer25_rece2.Text := (inttostr(v2));
        editexer25_rece3.Text := (inttostr(v3));
      end;
      if v3 > v2 then
      begin
        editexer25_rece1.Text := (inttostr(v1));
        editexer25_rece2.Text := (inttostr(v3));
        editexer25_rece3.Text := (inttostr(v2));
      end;
    end;
  end;
  if v2 > v1 then
  begin
    if v2 > v3 then
    begin
      if v1 > v3 then
      begin
        editexer25_rece1.Text := (inttostr(v2));
        editexer25_rece2.Text := (inttostr(v1));
        editexer25_rece3.Text := (inttostr(v3));
      end
      else
      begin
        editexer25_rece1.Text := (inttostr(v2));
        editexer25_rece2.Text := (inttostr(v3));
        editexer25_rece3.Text := (inttostr(v1));
      end;
    end;
  end;
  if v3 > v2 then
  begin
    if v3 > v1 then
    begin
      if v1 > v2 then
      begin
        editexer25_rece1.Text := (inttostr(v3));
        editexer25_rece2.Text := (inttostr(v1));
        editexer25_rece3.Text := (inttostr(v2));
      end;
      if v2 > v1 then
      begin
        editexer25_rece1.Text := (inttostr(v3));
        editexer25_rece2.Text := (inttostr(v2));
        editexer25_rece3.Text := (inttostr(v1));
      end;
    end;
  end;
end;

procedure TForm1.Button25Click(Sender: TObject);
var
v1,v2,v3: integer;
begin
v1:= strtoint(Editexer28_v1.Text);
v2:= strtoint(Editexer28_v2.Text);
v3:= strtoint(Editexer28_v3.Text);

  if (v1=v2) and (v1=v3) and (v2=v3) then
  begin
    ShowMessage('Temos um triangulo do tipo equiláteros: todos os lados concruentes');
  end
  else
  if (v1=v2) or (v1=v3) or (v2=v3) then
  begin
    ShowMessage('Temos um triangulo do tipo isosceles: dois lados concruentes');
  end
  else
  begin
    ShowMessage('Temos um triangulo do tipo ascaleno: todos os lados com medidas distintas');
  end;
end;


procedure TForm1.Button26Click(Sender: TObject);
var
v1,v2,v3,result: integer;
begin
v1:= strtoint(Editexer29_v1.Text);
v2:= strtoint(Editexer29_v2.Text);
v3:= strtoint(Editexer29_v3.Text);
result:= v1+v2+v3;

  if (result=90) then
  begin
    ShowMessage('OK, Temos um trinagulo retângulo');
    exit;
  end;
  if (result<90) then
  begin
    ShowMessage('OK, temos um trinagulo acuângulo');
  end
  else
  begin
    ShowMessage('OK, Temos um trinagulo obtusângulo');
  end;


end;

